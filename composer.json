{
  "name": "dealerinspire/cache-driver-failover",
  "description": "Use your preferred cache driver and failover when that driver is not available.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Nick Wilging",
      "email": "nwilging@dealerinspire.com"
    }
  ],
  "require": {
    "php": "^8.0.2",
    "laravel/framework": ">=9.0"
  },
  "autoload": {
    "psr-4": {
      "DealerInspire\\AppCache\\": "./src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DealerInspire\\AppCache\\AppCacheProvider"
      ]
    }
  },
  "require-dev": {
    "orchestra/testbench": "^5.0@dev",
    "mockery/mockery": "^1.0@dev",
    "phpunit/phpunit": "^8.4@dev"
  }
}
